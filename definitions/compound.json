{
    "variables": {
        "average_speed": {
            "crisp_universe": [0, 101, 1],
            "fuzzy_values": ["slow", "medium", "fast"]
        },
        "comfort": {
            "crisp_universe": [0, 11, 1],
            "fuzzy_values": ["low", "medium", "high"]
        },
        "journey_time": {
            "crisp_universe": [0, 11, 1],
            "fuzzy_values": ["short", "medium", "long"]
        },
        "driving": {
            "crisp_universe": [0, 11, 1],
            "fuzzy_values": ["bad", "average", "good"]
        },
        "tip": {
            "crisp_universe": [0, 26, 1],
            "fuzzy_values": ["small", "moderate", "big"]
        }
    },
    "rules": [{
        "if": [
            [
                ["average_speed", "fast"]
            ],
            [
                ["comfort", "high"], ["comfort", "medium"]
            ]
        ],
        "then": ["driving", "good"]
    }, {
        "if": [
            [
                ["driving", "good"]
            ],
            [
                ["journey_time", "short"]
            ]
        ],
        "then": ["tip", "big"]
    }, {
        "if": [
            [
                ["driving", "good"],
                ["driving", "average"]
            ],
            [
                ["journey_time", "medium"]
            ]
        ],
        "then": ["tip", "moderate"]
    }, {
        "if": [
            [
                ["driving", "bad"]
            ],
            [
                ["journey_time", "long"]
            ]
        ],
        "then": ["tip", "small"]
    }]
}
